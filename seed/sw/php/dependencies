#!/bin/bash
main() {
    echo "Installing dependencies for PHP build..."
    sudo apt-get install -y \
        re2c \
        libcurl4-openssl-dev \
        pkg-config \
        libpspell-dev \
        libicu-dev \
        libmagickwand-dev \
        libmcrypt-dev \
        libxslt1-dev # \
        #libgvc6 \
        #libxslt-dev # || true
}

main
